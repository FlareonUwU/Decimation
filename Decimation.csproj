<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="..\..\references\tModLoader.targets" />
  <PropertyGroup>
    <AssemblyName>Decimation</AssemblyName>
    <TargetFramework>net472</TargetFramework>
    <PlatformTarget>x86</PlatformTarget>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>
  <Target Name="BuildMod" AfterTargets="Build">
    <Exec Command="&quot;$(tMLBuildServerPath)&quot; -build $(ProjectDir) -eac $(TargetPath) -define $(DefineConstants) -unsafe $(AllowUnsafeBlocks)" />
  </Target>
  <Target Name="PostBuild" AfterTargets="PostBuildEvent">
    <Exec Command="&quot;D:\Jeux\steamapps\common\Tmod\tModLoaderServer.exe&quot; -build &quot;$(ProjectDir)\&quot;" />
  </Target>
  <ItemGroup>
    <Compile Remove="NPCs\AncientDuneWorm\AncientDuneWormBody.cs" />
    <Compile Remove="NPCs\AncientDuneWorm\AncientDuneWormHead.cs" />
    <Compile Remove="NPCs\AncientDuneWorm\AncientDuneWormTail.cs" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="NPCs\AncientDuneWorm\AncientDuneWorm" />
  </ItemGroup>
</Project>